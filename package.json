{
  "name": "tulp-ui",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && sass src/lib/style/components:dist/style --style compressed --no-source-map && sh scripts/clear-files.sh",
    "build:docs": "vue-tsc --noEmit && vite build --config docs-config.ts",
    "serve": "vite preview",
    "test": "jest src"
  },
  "dependencies": {
    "vue": "^3.2.6",
    "vue-router": "4.0.10"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.6",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/test-utils": "^2.0.0-rc.14",
    "jest": "26.6.3",
    "sass": "^1.37.5",
    "ts-jest": "26.5.6",
    "typescript": "^4.3.2",
    "vite": "^2.5.4",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.2.2"
  }
}
